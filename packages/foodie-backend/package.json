  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techeats/foodie.git"
  },
  "scripts": {
    "start": "node ./bin/www.js",
    "server": "nodemon ./bin/www.js",
    "build": "npm run webpack & npm run tsc",
    "serve": "concurrently --kill-others \"npm run tsc-watch\" \"npm run server\"",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "lint": "tslint src/**",
    "seed": "tsc && node ./bin/seeder.js",
    "init-jest": "jest --init",
    "pre-commit": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/techeats/foodie/issues"
  },
