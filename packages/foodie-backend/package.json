{
  "name": "foodie-backend",
  "version": "1.0.0",
  "description": "Foodie Backend and Api",
  "keywords": [
    "Nodejs",
    "Express",
    "RestApi",
    "Passport"
  ],
  "author": "melitus <asmelitus@gmail.com>",
  "homepage": "https://github.com/techeats/foodie#readme",
  "license": "ISC",
  "main": "lib/foodie-backend.js",
