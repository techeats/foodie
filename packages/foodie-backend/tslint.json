{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "package.json",
      "package-lock.json",
      "tsconfig.json",
      "tslint.json",
      "lib/*generated.js"
